{
  "name": "Example API Benchmark",
  "global": {
    "maxRequests": 200,
    "concurrent": 1,
    "timeout": 15000
  },
  "endpoints": [
    {
      "name": "Login",
      "url": "https://api.example.com/auth/login",
      "method": "POST",
      "maxRequests" : 1,
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "username": "testuser",
        "password": "testpass"
      },
      "variables": [
        {
          "name": "authToken",
          "path": "token",
          "from": "response"
        }
      ]
    },
    {
      "name": "Get User Profile",
      "url": "https://api.example.com/user/profile",
      "method": "GET",
      "headers": {
        "Authorization": "Bearer {{authToken}}"
      },
      "dependencies": [
        "Login"
      ]
    }
  ]
}